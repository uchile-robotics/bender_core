<launch> 
  <node name="gripper_test_manager" pkg="dynamixel_controllers" type="controller_manager.py" required="true" output="screen">
		<rosparam file="$(find bender_arm)/config/gripper_test_port.yaml" command="load"/>
	</node>

  <rosparam file="$(find bender_arm)/config/gripper_test_controller.yaml" command="load"/>

  <node name="gripper_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=gripper_test_manager
                --port=gripper_test_port
                int_finger_controller
                ext_finger_controller"
          output="screen"/>

  <rosparam file="$(find bender_arm)/config/gripper_test_action.yaml" command="load"/>

  <node name="gripper_test_controller_spawner_meta" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=gripper_test_manager
                --type=meta
                gripper_test_controller
                int_finger_controller
                ext_finger_controller"
          output="screen"/>

</launch>