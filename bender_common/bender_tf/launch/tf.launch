<launch>

	<arg name="use_gui" default="false"/>
	<arg name="model_type" default="asus"/>

	<!-- transform tree from model -->
	<include file="$(find bender_tf)/launch/tf_model.launch">
		<arg name="use_gui" value="$(arg use_gui)"/>
		<arg name="model_type" value="$(arg model_type)"/>
	</include>

	<!-- static transform tree -->
	<include file="$(find bender_tf)/launch/bender_static_tf.launch"/>

	<!-- transformers -->
	<include file="$(find bender_tf)/launch/transformers.launch"/>

</launch>